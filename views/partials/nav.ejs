  <% if (!user) { %>
    <link rel="stylesheet" href="/stylesheets/siwg.css">
  <% } %>
</head>
<body>
  <nav class="sticky">
    <a href="/"><button type="submit">Home</button></a>
    <a href="/creatures"><button type="submit">Creatures</button></a>
    <% if (user) { %>
      <a href="/profiles/<%= user.profile._id %> "><button type="submit">My Profile</button></a>
      <a href="/profiles"><button type="submit">Users</button></a>
      <a href="/auth/logout">
        <button type="submit">Logout</button>
      </a>
    <% } else { %>
      <a href="/auth/google" class="siwg-link">
        <button type="submit" class="siwg">
          <img id="siwg-image" alt="Sign in with Google" />
        </button>
      </a>
    <% } %>
  </nav>
